{
  "name": "instagram-auto-poster",
  "version": "3.0.0",
  "description": "Instagram Automated Post Management System",
  "main": "dist/server.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "tsx watch src/server.ts",
    "client:dev": "cd client && npm run dev",
    "build": "npm run build:server && npm run build:client",
    "build:server": "tsc && tsc-alias",
    "build:client": "cd client && npm install && npm run build",
    "start": "node dist/server.js",
    "start:prod": "pm2 start ecosystem.config.js",
    "worker": "node dist/workers/post.worker.js",
    "worker:dev": "tsx watch src/workers/post.worker.ts",
    "migrate": "prisma migrate deploy --schema=./src/prisma/schema.prisma",
    "migrate:dev": "prisma migrate dev --schema=./src/prisma/schema.prisma",
    "seed": "tsx src/database/seed.ts",
    "test": "jest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.10.2",
    "@types/cheerio": "^0.22.35",
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.3",
    "bullmq": "^5.63.2",
    "cheerio": "^1.1.2",
    "cloudinary": "^2.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "node-cron": "^3.0.3",
    "openai": "^4.28.0",
    "puppeteer": "^24.31.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "zod": "^3.22.4",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.11.24",
    "@types/node-cron": "^3.0.11",
    "@types/supertest": "^6.0.3",
    "concurrently": "^8.2.2",
    "jest": "^30.2.0",
    "prisma": "^5.22.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-jest": "^29.4.5",
    "tsx": "^4.7.1"
  }
}
