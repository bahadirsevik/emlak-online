NODE_ENV=production
PORT=3000
DATABASE_URL="postgresql://user:password@localhost:5432/instagram_poster?schema=public"
JWT_SECRET=your_production_secret_key_here
JWT_REFRESH_SECRET=your_production_refresh_secret_key_here

# Instagram / Facebook Graph API
INSTAGRAM_APP_ID=your_facebook_app_id
INSTAGRAM_APP_SECRET=your_facebook_app_secret
INSTAGRAM_REDIRECT_URI=https://your-domain.com/api/instagram/callback
ENCRYPTION_KEY=32_char_random_string_for_encryption

# DeepSeek API
DEEPSEEK_API_KEY=your_deepseek_key
DEEPSEEK_API_URL=https://api.aimlapi.com/v1
AI_MODEL=deepseek-chat
OPENAI_API_KEY=your_openai_key

# Cloudinary
CLOUDINARY_CLOUD_NAME=your_cloud_name
CLOUDINARY_API_KEY=your_api_key
CLOUDINARY_API_SECRET=your_api_secret
